{
  "compilerOptions": {
    "target": "es5",
    "skipLibCheck": true,
    "declaration": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "./"
  },
  "exclude": [
    "node_modules",
    "operations",
    "schema",
    "types",
    "utils",
    "codegen.ts"
  ]
}
